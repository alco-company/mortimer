<div id="<%= dom_id user %>">
  <p>
    <strong>Account:</strong>
    <%= user.account_id %>
  </p>

  <p>
    <strong>User name:</strong>
    <%= user.user_name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

  <p>
    <strong>Confirmed at:</strong>
    <%= user.confirmed_at %>
  </p>

  <p>
    <strong>Password digest:</strong>
    <%= user.password_digest %>
  </p>

  <p>
    <strong>Unconfirmed email:</strong>
    <%= user.unconfirmed_email %>
  </p>

  <p>
    <strong>Remember token:</strong>
    <%= user.remember_token %>
  </p>

  <p>
    <strong>Session token:</strong>
    <%= user.session_token %>
  </p>

  <p>
    <strong>Logged in at:</strong>
    <%= user.logged_in_at %>
  </p>

  <p>
    <strong>On task since at:</strong>
    <%= user.on_task_since_at %>
  </p>

</div>
