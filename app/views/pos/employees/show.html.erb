                    </thead>
                    <%= turbo_stream_from "employee_#{resource.id}_pupils"  %>
                    <%= turbo_frame_tag "employee_#{resource.id}_pupils" do %>
                      <%= render partial: "pos/employees/employee_pupils", locals: { resource: resource } %>
                    <% end %>
                  </table>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
