<p style="color: green"><%= notice %></p>

<h1 class="prose lg:prose-xl">Dashboards</h1>

<div id="dashboards">
  <% @dashboards.each do |dashboard| %>
    <%= render dashboard %>
    <p>
      <%= link_to "Show this dashboard", dashboard %>
    </p>
  <% end %>
</div>

<%= link_to t(:new), new_dashboard_path %>
